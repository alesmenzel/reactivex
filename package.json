{
  "name": "@alesmenzel/reactivex",
  "version": "1.1.1",
  "description": "Framework agnostic state management library",
  "license": "MIT",
  "main": "./dist/reactivex.js",
  "types": "./dist/types/index.d.ts",
  "author": "Ale≈° Menzel <alesmenzel@gmail.com> (https://github.com/alesmenzel)",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rollup --config rollup.config.js && tsc --project tsconfig.json",
    "cb": "run-s clean build",
    "lint": "eslint .",
    "format": "eslint --fix .",
    "pretest": "run-s lint types",
    "test": "npm run test:unit",
    "test:unit": "jest --ci --config ./jest/jest.config.js",
    "test:unit:watch": "jest --watchAll --config ./jest/jest.config.js",
    "test:coverage": "jest --ci --config ./jest/jest-coverage.config.js",
    "test:coverage:watch": "jest --coverage --watchAll -i --config ./jest/jest-coverage.config.js",
    "types": "tsc --project tsconfig.json",
    "prepublishOnly": "npm run test"
  },
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:alesmenzel/reactivex.git"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/eslint-plugin": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@babel/runtime": "^7.12.13",
    "@babel/runtime-corejs3": "^7.12.13",
    "@rollup/plugin-json": "^4.1.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/rollup-plugin-progress": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "core-js": "^3.8.3",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-unicorn": "^27.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.38.5",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-progress": "^1.1.2",
    "rollup-plugin-terser": "^7.0.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  }
